{
  "name": "hq",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "scss": "node-sass --recursive ./src/scss -o ./public/stylesheets",
    "dev": "concurrently \"nodemon ./src/bin/www \" \"node-sass --watch ./src/scss -o ./public/stylesheets \"  ",
    "db:create": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.165.0",
    "@aws-sdk/s3-request-presigner": "^3.165.0",
    "@vimeo/player": "^2.17.1",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "node-sass": "^7.0.1",
    "normalize.css": "^8.0.1",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "sequelize": "^6.21.3",
    "sib-api-v3-sdk": "^8.4.2",
    "uuid": "^9.0.0",
    "vimeo": "^2.1.1"
  }
}
